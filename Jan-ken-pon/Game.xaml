        <Button x:Name="Stone" Content="Stone" HorizontalAlignment="Left" Margin="20,210,0,0" VerticalAlignment="Top" Width="75" Click="Stone_Click"/>
        <Button x:Name="Scissors" Content="Scissors" HorizontalAlignment="Left" Margin="115,210,0,0" VerticalAlignment="Top" Width="75" Click="Scissors_Click"/>
        <Button x:Name="Paper" Content="Paper" HorizontalAlignment="Left" Margin="207,210,0,0" VerticalAlignment="Top" Width="75" Click="Paper_Click"/>
        <Button x:Name="Next" Content="Next round" HorizontalAlignment="Left" Margin="115,249,0,0" VerticalAlignment="Top" Width="75" Click="Next_Click"/>

    </Grid>
</Window>
